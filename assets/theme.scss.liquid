body.mmactive {
  overflow: hidden;
}
body.mmactive::after {
    background: rgba(255, 255, 255, 0.5);
    bottom: 0;
    content: "";
    display: block;
    height: 100%;
    left: 0;
    position: absolute;
    right: 0;
    top: 0;
    width: 100%;
    z-index: 5;
}
