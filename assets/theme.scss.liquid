    width: 1em;
    height: 1em;
    margin-top: -0.3em;
  }
}

.form-control{
  width:100%;
  height:40px;
  border-top:none;
  border-right:none;
  border-left:none;
  border-radius:0;
  padding:0;
  font-family:$nimbus;
  font-size:$input-field-font-size;
  font-weight:$regular;
  border-color:$black;
  &:focus{
    box-shadow:none;
    outline:none;
    border-color:$black;
  }
}

.btn{
  font-family:$nimbus;
  font-size:$btn-font-size;
  color:$black;
  font-weight:$regular;
  text-transform:$capital;
  letter-spacing:1.08px;
  border-radius:0;
  padding:0 13px;
  background:none;
  height:50px;
  line-height:50px;
  &.btn-border{
    height:40px;
    border:solid 1px $color-border;
    line-height:37px;
  }
  &.btn-dark{
    background:$black;
    color:$white;
  }
  &:hover, &:focus{
    opacity:1;
    box-shadow:none;
    background:$black;
    color:$white;
  }
  @include media-query($small){
    height:40px;
    line-height:40px;
    &.btn-border{
      height:40px;
      line-height:40px;
    }
  }
}

select,select.form-control{
  background:#ffffff url('{{ "droparrow.svg" | asset_url }}') no-repeat calc(100% - 15px) center;
  background-size:15px;
  text-overflow:ellipsis;
  border:none;
  white-space:nowrap;
  padding-right:28px;
  -moz-appearance:none;
  -webkit-appearance:none;
  -ms-appearance:none;
  -o-appearance:none;
  appearance:none;
  width:100%;
}

