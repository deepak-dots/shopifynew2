<!-- START of Footer [footer.liquid] -->
<!-- JSON-LD markup generated by Google Structured Data Markup Helper. -->
<script type="application/ld+json">
[ {
  "@context" : "http://schema.org",
  "@type" : "LocalBusiness",
  "name" : "Suzannah Boutique",
  "telephone" : "{{ section.settings.address_block_4 | strip_html }}",
  "image" : "https://cdn.shopify.com/s/files/1/0068/0873/1721/files/20180725-8050_617x411@2x.JPG",
  "address" : {
    "@type" : "PostalAddress",
    "streetAddress" : "{{ section.settings.address_block_2 | strip_html }}"
  }
}, {
  "@context" : "http://schema.org",
  "@type" : "LocalBusiness",
  "name" : "Suzannah: The Atelier",
  "image" : "https://cdn.shopify.com/s/files/1/0068/0873/1721/files/the-atelier5.jpg",
  "telephone" : "{{ section.settings.address_block_4 | strip_html }}",
  "address" : {
    "@type" : "PostalAddress",
    "streetAddress" : "{{ section.settings.address_block_3 | strip_html }}"
  }
} ]
</script>
{%- assign inline_list = false -%}
<footer class="footer style-four">
  <div class="container">
    <div class="footer-top clearfix">
      <div class="row">
        <div class="col-12 col-sm-12 col-md-3">
          {%- if section.settings.title_block_1 != blank -%}
            <b>{{ section.settings.title_block_1 }}</b>
          {%- endif -%}
          {%- if section.settings.address_block_1 != blank -%}
            {{ section.settings.address_block_1 }}
          {%- endif -%}
        </div>
        <div class="col-12 col-sm-12 col-md-9 col-lg-8 ml-auto">
          <div class="row">
            <div class="col-12 col-sm-6 col-md-4">
              <div class="wd-foot">
                {%- if section.settings.title_block_2 != blank -%}
                  <b>{{ section.settings.title_block_2 }}</b>
                {%- endif -%}
                {%- if section.settings.address_block_2 != blank -%}
                  {{ section.settings.address_block_2 }}
                {%- endif -%}
              </div>
            </div>
            <div class="col-12 col-sm-6 col-md-4">
              <div class="wd-foot">
                {%- if section.settings.title_block_3 != blank -%}
                  <b>{{ section.settings.title_block_3 }}</b>
                {%- endif -%}
                {%- if section.settings.address_block_3 != blank -%}
                  {{ section.settings.address_block_3 }}
                {%- endif -%}
              </div>
            </div>
            <div class="col-12 col-sm-6 col-md-4">
              <div class="row">
                <div class="col-12">
                  <p><a href="tel:{{ settings.site_telephone }}" aria-describedby="a11y-external-message">{{ settings.site_telephone }}</a>
                  <br><a href="tel:{{ settings.site_mobile }}" aria-describedby="a11y-external-message">{{ settings.site_mobile }}</a></p>
                </div>
                <div class="col-12">
                  {%- include 'social-icons' -%}
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
    <div class="row">
      <div class="col-12 col-sm-12 col-md-12 col-lg-3">
        <div class="newsletter">
          {%- if section.settings.text != blank -%}
          {{ section.settings.text}}
          {%- endif -%}
          {% unless canonical_url == 'https://www.suzannah.com/challenge' %}
          <div class="newsletter-field">
            {%- assign formId = 'ContactFooter' -%}
            {%- if section.settings.contact_form_type == 'sign_up_to' -%}
              {%- include 'contact_form_signupto' -%}
            {%- else -%}
              {%- include 'contact_form_shopify' -%}
            {%- endif -%}
          </div>
          {% endunless %}
        </div>
      </div>
      <div class="col-12 col-sm-12 col-md-12 col-lg-8 ml-auto">
        <div class="row">
          {%- for block in section.blocks -%}
            {%- assign footer_linklist = block.settings.menu -%}
            <div class="col-12 col-sm-6 col-md-4">
              <ul class="foot-link">
                {%- for link in linklists[footer_linklist].links -%}
                  <li class="site-footer__linklist-item">
                    <a href="{{ link.url }}">{{ link.title }}</a>
                  </li>
                {%- endfor -%}
              </ul>
            </div>
          {%- endfor -%}
          <!--=====footermenuaccording=======-->
           <div class="custom-accordin">
              <div id="accordion" class="accordion">
                 <div class="card mb-0">
                   {% assign j = 0 %}
                   {%- for block in section.blocks -%}
                       {%- if block.type == 'mobile_footer' -%}
                       {%- assign footer_linklist = block.settings.menu_mobile -%}
                         <a class="card-title card-header collapsed" data-toggle="collapse" href="#collapse_{{j}}">
                           <span class="">{{block.settings.menu_title}} </span>
                         </a>
                         <div id="collapse_{{j}}" class="card-body collapse" data-parent="#accordion" >
                           <ul class="foot-mobile-nav">
                               {%- for link in linklists[footer_linklist].links -%}
                                     <li><a href="{{ link.url }}">{{ link.title }}</a></li>
                               {%- endfor -%}
                           </ul>
                         </div>
                       {% assign j = j | plus: 1 %}
                       {%- endif -%}
                   {%- endfor -%}
                 </div>
              </div>
           </div>
         <!--=====footermenuaccording=======-->
          <div class="mobile-address-block clearfix">
            <div class="footer-mobile-block-1">
              {%- if section.settings.title_block_1 != blank -%}
                <b>{{ section.settings.title_block_1 }}</b>
              {%- endif -%}
              {%- if section.settings.address_block_1 != blank -%}
                {{ section.settings.address_block_1 }}
              {%- endif -%}
            </div>
            <div class="footer-mobile-block-2">
              {%- if section.settings.title_block_2 != blank -%}
                <b>{{ section.settings.title_block_2 }}</b>
              {%- endif -%}
              {%- if section.settings.address_block_2 != blank -%}
                {{ section.settings.address_block_2 }}
              {%- endif -%}
            </div>
            <div class="footer-mobile-block-2">
              {%- if section.settings.title_block_3 != blank -%}
                <b>{{ section.settings.title_block_3 }}</b>
              {%- endif -%}
              {%- if section.settings.address_block_3 != blank -%}
                {{ section.settings.address_block_3 }}
              {%- endif -%}
            </div>
          </div>
          <div class="mobile-foot-bottom clearfix">
            <div class="row no-gutters">
              <div class="col-6">
                {%- include 'social-icons-mobile' -%}
              </div>
              <div class="col-6">
                <ul class="mm-social right-link text-right">
                  <li><a href="tel:{{ settings.site_telephone }}" aria-describedby="a11y-external-message">{{ settings.site_telephone }}</a></li>
                </ul>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</footer>
