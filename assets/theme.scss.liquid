:-moz-placeholder {color: $black!important; opacity:1;}
::-moz-placeholder {color: $black!important; opacity:1;}
:-ms-input-placeholder {color: $black!important; opacity:1;}

.icon {
  display: inline-block;
  width: 18px;
  height: 18px;
  vertical-align: middle;
  fill: currentColor;
  .no-svg & {
    display: none;
  }
}

svg,
symbol {
  &.icon:not(.icon--full-color) {
    circle,
    ellipse,
    g,
    line,
    path,
    polygon,
    polyline,
    rect {
      fill: inherit;
      stroke: inherit;
    }
  }
}
